class Solution {
public:
    int sumFourDivisors(vector<int>& nums) {
        int totalsum=0;
        for(int i=0;i<nums.size();i++){
        int count=0;
        int sum=0;
        int n=nums[i];
        for(int i=1;i<=n;i++){
            if(n%i==0){
                count++;
                sum+=i;
            }
            if(count>4)break;
        }
            if(count==4){
                sum += sum;
            }
        }
        return totalsum;
    }
};
